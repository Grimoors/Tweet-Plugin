    outline: none;\r\n    resize: none;\r\n    font-size: 1.2rem;\r\n    font-weight: 400;\r\n    color: #14171a;\r\n}\r\n\r\n.tweet-box-footer{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding-bottom: auto;\r\n}\r\n\r\n.tweet-box-footer-left{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.tweet-box-footer-left-icons{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.tweet-box-footer-left-icons i{\r\n    font-size: 1.5rem;\r\n    color: #5b7083;\r\n    margin-right: 10px;\r\n    cursor: pointer;\r\n}\r\n\r\n.tweet-box-footer-right{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.tweet-box-footer-right-char-count{\r\n    margin-right: 10px;\r\n}\r\n\r\n.tweet-box-footer-right-char-count h6{\r\n    font-size: 1rem;\r\n    font-weight: 400;\r\n    color: #5b7083;\r\n}\r\n\r\n.tweet-box-footer-right-tweet-btn{\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.tweet-box-footer-right-tweet-btn button{\r\n    width: 100px;\r\n    height: 40px;\r\n    background-color: #1da1f2;\r\n    border: none;\r\n    outline: none;\r\n    border-radius: 20px;\r\n    color: #fff;\r\n    font-size: 1rem;\r\n    font-weight: 600;\r\n    cursor: pointer;\r\n}\r\n\r\n.tweet-box-footer-right-tweet-btn button:hover{\r\n    background-color: #1a91da;\r\n}\r\n\r\n.tweet-box-footer-right-tweet-btn button:active{\r\n    background-color: #1a91da;\r\n}\r\n\r\n.metrics {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n    margin-top: 20px;\r\n}\r\n\r\n.metrics-box {\r\n    width: 200px;\r\n    height: 200px;\r\n    background-color: #1da1f2;\r\n    border-radius: 10px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n    align-items: center;\r\n}\r\n\r\n.metrics-box-header {\r\n    width: 100%;\r\n    height: 50px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.metrics-box-header h3 {\r\n    color: white;\r\n    font-size: 1.5rem;\r\n}\r\n\r\n.metrics-box-body {\r\n    width: 100%;\r\n    height: 100px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.metrics-box-body h1 {\r\n    color: white;\r\n    font-size: 3rem;\r\n}\r\n\r\n/* Font Awesome CSS */\r\n@import url('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css');\r\n"},"lineCount":null}},"error":null,"hash":"ea22cc3edfa0ce03633d4d26ab1ef28c","cacheData":{"env":{}}}